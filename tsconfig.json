{
  "compilerOptions": {
    "target": "es2015",                      // ðŸ‘ˆ ADICIONE ISSO
    "lib": ["dom", "dom.iterable", "esnext"],
    "downlevelIteration": true,             // ðŸ‘ˆ ADICIONE ISSO
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
